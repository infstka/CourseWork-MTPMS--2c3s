<Window x:Class="LineUp.Groups.DelMember"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LineUp.Groups"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"       
        Title="LineUp/Delete member" Height="314.127" Width="254.017" Icon="icon.png">
    <Grid Style="{DynamicResource Grid}">
        <ComboBox x:Name="MembersList" HorizontalAlignment="Left" Margin="50,52,0,0" VerticalAlignment="Top" Width="160" Style="{DynamicResource Combo}"/>
        <Button x:Name="ConfirmBtn" Content="{DynamicResource Delete}" HorizontalAlignment="Left" Margin="50,170,0,0" VerticalAlignment="Top" Width="160" Click="ConfirmBtn_Click" Style="{DynamicResource Button}"/>
    </Grid>
</Window>
