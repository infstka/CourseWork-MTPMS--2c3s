<Window x:Class="LineUp.Groups.AddSong"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LineUp.Groups"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"       
        Title="LineUp/Add song" Height="335.319" Width="330.055">
    <Grid Style="{DynamicResource Grid}">
        <TextBlock HorizontalAlignment="Left" Margin="50,45,0,0" TextWrapping="Wrap" Text="{DynamicResource TName}" VerticalAlignment="Top" Style="{DynamicResource TextBlock}" Width="135" Height="25"/>
        <TextBox x:Name="SongName" HorizontalAlignment="Left" Height="25" Margin="50,90,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="135" Style="{DynamicResource TextBox}"/>
        <Button x:Name="ConfirmBtn" Content="{DynamicResource Add}" HorizontalAlignment="Left" Margin="50,133,0,0" VerticalAlignment="Top" Width="135" Click="ConfirmBtn_Click" Height="25" Style="{DynamicResource Button}"/>

    </Grid>
</Window>
