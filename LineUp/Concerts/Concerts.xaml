<UserControl x:Class="LineUp.Concerts.Concerts"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LineUp.Concerts"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label x:Name="TimerSeconds" Content="0" HorizontalAlignment="Left" Margin="54,350,0,0" VerticalAlignment="Top" FontSize="48" Style="{DynamicResource Label}"/>
        <Label x:Name="TimerMinutes" Content="0" HorizontalAlignment="Left" Margin="54,287,0,0" VerticalAlignment="Top" FontSize="48" Style="{DynamicResource Label}"/>
        <Label x:Name="TimerHours" Content="0" HorizontalAlignment="Left" Margin="48,220,0,0" VerticalAlignment="Top" FontSize="48" Style="{DynamicResource Label}"/>
        <Label x:Name="TimerDays" Content="0" HorizontalAlignment="Left" Margin="54,128,0,0" VerticalAlignment="Top" FontSize="48" Style="{DynamicResource Label}"/>
        <DatePicker x:Name="DateChoose" HorizontalAlignment="Left" Margin="444,128,0,0" VerticalAlignment="Top"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="354,330,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Style="{DynamicResource Button}"/>

    </Grid>
</UserControl>
